{
  "arguments": {
    "key": {
      "type": "String"
    }
    , "port": {
      "type": "Integer"
    }
  },
  "sourceEndpoints": {
    "MS SQL Server": {
      "type": "SQL",
      "url": "$sqlserver://localhost:<port>/test",
      "username": "sa",
      "password": "T0p-secret"
    },
    "MySQL": {
      "type": "SQL",
      "url": "$mysql://localhost:<port>/test",
      "username": "user",
      "password": "T0p-secret"
    }
    ,
    "PostgreSQL": {
      "type": "SQL",
      "url": "$postgresql://localhost:<port>/test",
      "username": "postgres",
      "password": "T0p-secret"
    }
  }
  ,
  "source": {    
    "type": "SQL",
    "endpoint": "$<key>",
    "streamingFetchSize": 7,
    "query": "select d.id, d.instant, l.value as ref, d.value from testData d join testRefData l on d.lookup = l.id where d.id < 20 order by d.id"
  }
  ,
  "processors": [
    {
      "type":"GROUP_CONCAT",
      "delimiter":",",
      "parentId":"id",
      "innerJoin":false,
      "idColumn":"dataid",      
      "columnName":"children",      
      "childColumnName":"value",      
      "source": {    
        "type": "SQL",
        "endpoint": "$<key>",
        "streamingFetchSize": 10,
        "query": "select dataId, value from testManyData m join testRefData r on r.id = m.refId where dataId < 20 order by dataId, sort"
      }
    }
    , 
    {
      "type":"LIMIT",
      "limit":13
    }
  ]
  ,
  "destination": {
    "type": "Logger"
  }
}